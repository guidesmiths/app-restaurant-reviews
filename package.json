{
	"name": "app-restaurant-reviews",
	"version": "0.0.1",
	"description": "...",
	"main": "index.js",
	"engines": {
		"node": "10.x"
	},
	"scripts": {
		"local": "cross-env SERVICE_ENV=local node index.js",
		"start": "node index.js",
		"start-app": "cd react-app && npm run dev",
		"build-app": "cd react-app && npm run build",
		"ci:up": "docker-compose --file ./docker/cicd/docker-compose.yml pull && docker-compose --file ./docker/cicd/docker-compose.yml up -d --build --force-recreate",
		"ci:down": "docker-compose --file ./docker/cicd/docker-compose.yml down",
		"test:ci": "cross-env SERVICE_ENV=test mocha --reporter spec './test/**/*.test.js'",
		"test": "cross-env SERVICE_ENV=test mocha --reporter spec './test/**/*.test.js'",
		"lint": "node_modules/.bin/eslint . && cd react-app && npm run lint",
		"manifest": "node_modules/make-manifest/bin/make-manifest",
		"qa": "npm run lint && npm run test",
		"precommit": "npm run lint",
		"prepush": "npm run test"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^16.0.0",
		"babel-eslint": "^9.0.0",
		"chai": "^4.2.0",
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^11.3.2",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-mocha": "^5.2.0",
		"expect.js": "^0.3.1",
		"husky": "^1.1.0",
		"i": "^0.3.7",
		"mocha": "^5.2.0",
		"mocha-junit-reporter": "^1.18.0",
		"npm": "^6.14.8",
		"prettier": "^1.19.1",
		"sinon": "^7.3.2",
		"supertest": "^3.0.0"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"body-parser": "^1.15.2",
		"boom": "^3.2.2",
		"bunyan": "^1.8.5",
		"chalk": "^1.1.3",
		"confabulous": "^1.1.0",
		"debug": "^2.2.0",
		"dotenv": "^8.0.0",
		"express": "^4.17.1",
		"express-swagger-generator": "^1.1.15",
		"hogan.js": "^3.0.2",
		"make-manifest": "^1.0.1",
		"on-headers": "^1.0.1",
		"optimist": "^0.6.1",
		"optional": "^0.1.3",
		"prepper": "^1.1.0",
		"ramda": "^0.26.1",
		"react-redux": "^7.1.3",
		"redux-logger": "^3.0.6",
		"systemic": "^3.0.0",
		"systemic-domain-runner": "^1.1.0",
		"systemic-express": "^1.1.0"
	}
}
